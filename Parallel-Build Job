pipeline {
    agent any
       stage('Parallel-Build Job') {
            steps {
                parallel(
                  development: {
                        sh 'echo Running development job'
                    },
                    production: {
                        sh 'echo Running production job'
                    },
                )
              }
            }
 post {
        always {
            echo 'Cleanup after everything!'
        }
    }
} 

