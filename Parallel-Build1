pipeline {
    agent any
    stages {
        stage('Parallel-Build1 Job') {
            parallel {
                stage ('development') {
                    steps {
                        sh 'echo Running development job'
                    }
                }
                stage ('production') {
                    steps {
                        sh 'echo Running production job'
                    }
                }
            }
        }
         }
}
 post {
        always {
            echo 'Cleanup after everything!'
        }
    }
