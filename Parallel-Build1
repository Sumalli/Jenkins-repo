pipeline {
    agent any
    stages {
       stage('Parallel-Build1') {
            parallel (
                stage ('development') {
                    steps {
                        sh 'echo Running development job'
                    }
                }
                stage ('production') {
                    steps {
                        sh 'echo Running production job'
                    }
                }
              
            )
        }
    }
}
